{
  "version": "0.0.0",
  "name": "vocabulary-helper",
  "author": "Thinker-ljn",
  "packageManager": "pnpm@6.32.3",
  "description": "A Tampermonkey Plugin for search vocabulary.",
  "tampermonkey": {
    "name": "vocabulary-helper",
    "description": "A Tampermonkey Plugin for search vocabulary.",
    "namespace": "",
    "match": [
      "https://www.vocabulary.com/*"
    ],
    "connect": [
      "www.vocabulary.com"
    ],
    "resource": [],
    "run-at": "document-body",
    "grant": [
      "GM.xmlHttpRequest"
    ]
  },
  "scripts": {
    "build": "vite build",
    "release": "node scripts/release.js",
    "dev": "vite --port 3001",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "test": "vitest --passWithNoTests"
  },
  "dependencies": {
    "@vueuse/core": "^8.7.4",
    "axios": "^0.26.1",
    "axios-userscript-adapter": "^0.2.0",
    "naive-ui": "^2.30.4",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.7",
    "@iconify-json/carbon": "^1.1.5",
    "@iconify-json/ion": "^1.1.2",
    "@types/node": "^17.0.45",
    "@types/tampermonkey": "^4.0.5",
    "@unocss/reset": "^0.31.17",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "^2.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "chalk": "^4.1.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.18.0",
    "eslint-config-greasemonkey": "^1.0.1",
    "execa": "^5.1.1",
    "jsdom": "^19.0.0",
    "minimist": "^1.2.6",
    "pnpm": "^6.32.23",
    "rollup-plugin-visualizer": "^5.6.0",
    "semver": "^7.3.7",
    "typescript": "^4.7.4",
    "unocss": "^0.31.17",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.12",
    "vite-plugin-pages": "^0.22.0",
    "vite-plugin-tampermonkey": "1.1.1",
    "vitest": "^0.9.4",
    "vue-tsc": "^0.34.17"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
